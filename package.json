    "yarn-deduplicate": "^6.0.2"
  },
  "dependencies": {
    "@apollo/client": "^3.8.8",
    "@apollo/server": "^4.10.0",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
