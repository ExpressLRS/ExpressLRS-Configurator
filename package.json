    "@mui/icons-material": "^5.15.3",
    "@mui/material": "^5.15.3",
    "@octokit/rest": "^20.0.2",
    "autosuggest-highlight": "^3.2.1",
    "bluejay-rtttl-parse": "^2.0.2",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "cross-spawn": "^7.0.3",
    "electron-debug": "^3.2.0",
